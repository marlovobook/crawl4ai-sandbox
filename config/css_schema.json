{
  "name": "GenericContentExtractor",
  "baseSelector": "body",
  "fields": [
    {
      "name": "all_headings",
      "selector": "h1, h2, h3, h4, h5, h6",
      "type": "text",
      "multiple": true
    },
    {
      "name": "all_paragraphs",
      "selector": "p",
      "type": "text",
      "multiple": true
    },
    {
      "name": "all_text_content",
      "selector": "div, span, section, article",
      "type": "text",
      "multiple": true
    },
    {
      "name": "all_links",
      "selector": "a[href]",
      "type": "text",
      "multiple": true
    },
    {
      "name": "all_link_urls",
      "selector": "a[href]",
      "type": "attribute",
      "attribute": "href",
      "multiple": true
    }
  ]
}